window/size/viewport_height=1080

[dotnet]

project/assembly_name="GodotBlockchain"

[editor_plugins]

enabled=PackedStringArray("res://addons/godotblockchain/plugin.cfg")

[gui]

theme/custom="res://ui/ui_theme.tres"
theme/custom_font="res://ui/fonts/Satoshi_Complete/Fonts/OTF/Satoshi-Bold.otf"
